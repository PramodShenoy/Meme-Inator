<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yorozuyastudios.pro.com.meme_inator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="yorozuyastudios.pro.com.meme_inator.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="yorozuyastudios.pro.com.meme_inator.MainActivity" />
        <activity android:name="yorozuyastudios.pro.com.meme_inator.ChooseActivity" />
        <activity android:name="yorozuyastudios.pro.com.meme_inator.EnterTextActivity" />
        <activity android:name="yorozuyastudios.pro.com.meme_inator.MemeListActivity" />
    </application>

</manifest>